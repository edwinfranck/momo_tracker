{
  "expo": {
    "name": "Djai",
    "slug": "momo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/djai_icon.png",
    "scheme": "momo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.edwind4.momo"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#DEFCD2",
        "foregroundImage": "./assets/images/ic_launcher_foreground.png",
        "backgroundImage": "./assets/images/ic_launcher_background.png",
        "monochromeImage": "./assets/images/ic_launcher_monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.edwind4.momo",
      "permissions": [
        "READ_SMS",
        "RECEIVE_SMS",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/djai_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}